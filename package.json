{
  "name": "flex-docs",
  "description": "Sharetribe Flex documentation site",
  "version": "1.0.0",
  "license": "Apache License, Version 2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sharetribe/flex-docs"
  },
  "private": true,
  "scripts": {
    "clean": "rm -rf .cache public",
    "build": "gatsby build --prefix-paths",
    "netlify-build": "yarn run build && yarn run netlify-postbuild",
    "netlify-postbuild": "./netlify-postbuild.sh",
    "dev": "gatsby develop",
    "serve": "gatsby serve --prefix-paths",
    "prod": "export NODE_ENV=production && yarn run build && yarn run serve",
    "format": "yarn run format-js && yarn run format-config && yarn run format-docs",
    "format-js": "prettier --write \"src/**/*.js\"",
    "format-config": "prettier --write \"gatsby-config.js\" \"gatsby-node.js\" \"gatsby-browser.js\"",
    "format-docs": "prettier --print-width=72 --write \"src/**/*.md\" \"docs/**/*.md\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.15.13",
    "gatsby-image": "^2.2.18",
    "gatsby-plugin-catch-links": "^2.1.9",
    "gatsby-plugin-google-tagmanager": "^2.1.9",
    "gatsby-plugin-manifest": "^2.2.16",
    "gatsby-plugin-react-helmet": "^3.1.7",
    "gatsby-plugin-sharp": "^2.2.21",
    "gatsby-plugin-sitemap": "^2.2.12",
    "gatsby-plugin-styled-components": "^3.1.5",
    "gatsby-remark-autolink-headers": "^2.1.10",
    "gatsby-remark-images": "^3.1.21",
    "gatsby-remark-prismjs": "^3.3.13",
    "gatsby-remark-reading-time": "^1.0.1",
    "gatsby-source-filesystem": "^2.1.22",
    "gatsby-transformer-remark": "^2.6.22",
    "gatsby-transformer-sharp": "^2.2.14",
    "github-slugger": "^1.2.1",
    "no-scroll": "^2.1.1",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-modal": "^3.10.1",
    "sanitize.css": "^11.0.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  }
}
